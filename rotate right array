class Solution {
    public void rotate(int[] nums, int k) {
        int i,l,j,n;
        n=nums.length;
        for(j=0;j<k;j++)
        {
         l=nums[n-1];
         for (i=n-1;i>0;i--)
            { 
                nums[i]=nums[i-1];
            }
          nums[0]=l;
        }
        
    }
}
