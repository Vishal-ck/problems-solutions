class Solution{
    public int rangeOR(int n){
        // Code here. 
        int c=0,s=0;
        while ((n/2) !=0)
        {
            c++;
            n=n/2;
        }
        for (int i=0;i<=c;i++)
        {
            s=s+(int)(Math.pow(2,i));            
        }
        return s;
    }
}
